<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DiagnosticsPage"
    android:background="#1f38be">

    <ProgressBar
        android:id="@+id/progressBar4"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="388dp"
        android:layout_height="511dp"
        android:layout_marginBottom="181dp"
        android:layout_marginLeft="11dp"
        android:layout_marginStart="11dp"
        android:layout_marginTop="50dp"
        android:indeterminate="false"
        android:max="50"
        android:progress="40"
        android:progressDrawable="@drawable/circular_progress_bar_background"
        android:rotation="215"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/linearLayout4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ProgressBar
        android:id="@+id/progressBar6"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="388dp"
        android:layout_height="511dp"
        android:layout_marginBottom="272dp"
        android:layout_marginLeft="11dp"
        android:layout_marginStart="11dp"
        android:layout_marginTop="50dp"
        android:indeterminate="false"
        android:max="50"
        android:progress="33"
        android:progressDrawable="@drawable/circular_progress"
        android:rotation="215"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/linearLayout4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:background="@drawable/bottom"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout7">

        <TextView
            android:id="@+id/textView12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/redhatdisplayregular"
            android:paddingLeft="20dp"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            android:text="Common Symptoms"
            android:textColor="#000000"
            android:textSize="25dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="125dp"
                android:layout_height="match_parent"
                android:src="@drawable/night" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fontFamily="@font/redhatdisplayregular"
                android:gravity="center_horizontal"
                android:paddingRight="50dp"
                android:paddingTop="17dp"
                android:text="Poor Night Vision"
                android:textAlignment="center"
                android:textColor="#000000"
                android:textSize="25dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="125dp"
                android:layout_height="match_parent"
                android:src="@drawable/cloud" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fontFamily="@font/redhatdisplayregular"
                android:gravity="center_horizontal"
                android:paddingRight="50dp"
                android:paddingTop="17dp"
                android:text="Cloudy Vision"
                android:textAlignment="center"
                android:textColor="#000000"
                android:textSize="25dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="125dp"
                android:layout_height="match_parent"
                android:src="@drawable/blur" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fontFamily="@font/redhatdisplayregular"
                android:gravity="center_horizontal"
                android:paddingRight="50dp"
                android:paddingTop="17dp"
                android:text="Blurry Vision"
                android:textAlignment="center"
                android:textColor="#000000"
                android:textSize="25dp" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="125dp"
                android:layout_height="match_parent"
                 />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fontFamily="@font/redhatdisplayregular"
                android:gravity="center_horizontal"
                android:paddingRight="50dp"
                android:paddingTop="17dp"

                android:textAlignment="center"
                android:textColor="#000000"
                android:textSize="25dp" />
        </LinearLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="96dp"
        android:fontFamily="@font/redhatdisplaybold"
        android:text="We are about"
        android:textColor="#ffffff"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout4" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/redhatdisplaybold"
        android:text="100% sure"
        android:textColor="#ffffff"
        android:textSize="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <LinearLayout
        android:id="@+id/linearLayout7"
        android:layout_width="303dp"
        android:layout_height="45dp"
        android:layout_marginTop="106dp"
        android:background="@drawable/bottom2"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10">

        <ImageView
            android:layout_width="71dp"
            android:layout_height="match_parent"
            android:paddingBottom="2dp"
            android:src="@drawable/hospital"
            tools:layout_editor_absoluteX="56dp"
            tools:layout_editor_absoluteY="417dp" />

        <TextView
            android:id="@+id/font"
            android:layout_width="261dp"
            android:layout_height="match_parent"
            android:fontFamily="@font/redhatdisplayregular"
            android:gravity="center_horizontal"
            android:paddingRight="60dp"
            android:paddingTop="12dp"
            android:text="Please meet with a specialist "
            android:textAlignment="center"
            android:textColor="#D2FFFFFF"
            android:textSize="15dp"
            tools:layout_editor_absoluteX="120dp"
            tools:layout_editor_absoluteY="417dp" />


    </LinearLayout>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton2"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_marginLeft="322dp"
        android:layout_marginStart="322dp"
        android:layout_marginTop="490dp"
        android:backgroundTint="#FF0099"
        android:clickable="true"
        android:scaleType="center"
        app:fabCustomSize="61dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:maxImageSize="37dp"
        app:srcCompat="@drawable/next" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="45dp"
        android:fontFamily="@font/redhatdisplaybold"
        android:text="YOU MAY HAVE"
        android:textColor="#ffffff"
        android:textSize="35dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="match_parent"
        android:layout_height="57dp"
        android:layout_marginTop="18dp"
        android:background="@drawable/diseasename"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13">

        <TextView
            android:id="@+id/textView14"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/redhatdisplaybold"
            android:gravity="center_horizontal"
            android:paddingRight="7dp"
            android:paddingTop="6dp"
            android:text="CATERACTS"
            android:textAlignment="center"
            android:textColor="#ffffff"
            android:textSize="35dp"
            android:textStyle="italic"
            tools:layout_editor_absoluteX="150dp"
            tools:layout_editor_absoluteY="101dp" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>